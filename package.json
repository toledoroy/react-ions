{
  "name": "react-ions",
  "version": "3.1.1",
  "description": "An open source set of React components that implement Ambassador's Design and UX patterns.",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/GetAmbassador/react-ions.git"
  },
  "engines": {
    "node": "4.8.4",
    "npm": "3.10.10"
  },
  "keywords": [
    "react",
    "conventions",
    "components",
